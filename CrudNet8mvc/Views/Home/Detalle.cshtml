@model CrudNet8mvc.Models.Contacto
@{
    ViewData["Title"] = "Detalle contacto";
}

<div class="row justify-content-center">
    <div class="col-md-8">
        <div class="card border">
            <div class="card-header bg-black">
                <h3 class="text-white"><strong>Editar usuario</strong></h3>
            </div>
            <div class="card-body">
                <ul class="list-group">
                    <li class="list-group-item"><strong>Nombre: </strong>@Html.DisplayFor(m => m.Nombre)</li>
                    <li class="list-group-item"><strong>Telefono: </strong>@Html.DisplayFor(m => m.Telefono)</li>
                    <li class="list-group-item"><strong>Emnail: </strong>@Html.DisplayFor(m => m.Email)</li>
                    <li class="list-group-item"><strong>Fecha: </strong>@Html.DisplayFor(m => m.FechaDeCreacon)</li>

                </ul>


                <a asp-controller="Home" asp-action="Index">Back</a>
            </div>
        </div>
    </div>
</div>
